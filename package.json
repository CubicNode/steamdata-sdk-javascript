{
  "name": "@cubicnode/steamdata-sdk-javascript",
  "version": "1.0.0",
  "description": "Steamdata SDK for javascript",
  "scripts": {
    "test": "vitest",
    "test:watch": "npm run test -- --watch --ui",
    "dev": "npm run test:watch",
    "lint": "eslint",
    "build-only": "unbuild",
    "build": "npm run lint && tsc -p tsconfig.main.json --noEmit && npm run build-only"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "files": [
    "./dist",
    "./src"
  ],
  "keywords": [
    "cubicnode",
    "steamdata",
    "sdk",
    "typescript",
    "javascript"
  ],
  "license": "Apache-2.0",
  "author": "Cubicnode company steamdata project team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CubicNode/steamdata-sdk-javascript"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.4.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@types/node": "^22.10.7",
    "@vitest/coverage-v8": "^3.0.1",
    "@vitest/ui": "^3.0.1",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "unbuild": "^3.3.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.1"
  }
}
